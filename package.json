{
  "name": "xas-thickness-calculator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "python xraylib_server.py",
    "dev:all": "concurrently -n \"API,WEB\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev:api\" \"npm run dev\"",
